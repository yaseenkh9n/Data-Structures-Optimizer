   <string notr="true">/* Main Window - Deep Space Gradient (matches dashboard) */
NewAnalysis {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #0d1117,
        stop:0.5 #151b25,
        stop:1 #0d1117
    );
}

/* Content Area */
#contentArea {
    background: transparent;
}

/* Page Title */
#pageTitle {
    color: #ffffff;
    font-size: 32px;
    font-weight: bold;
}

#pageSubtitle {
    color: #8b92a7;
    font-size: 14px;
}

/* Group Boxes */
QGroupBox {
    background-color: #1f2333;
    border: 1px solid #2a2d3e;
    border-radius: 12px;
    padding: 12px;
    margin-top: 15px;
    color: #ffffff;
    font-size: 14px;
    font-weight: bold;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 3px 10px;
    background-color: #1f2333;
    border-radius: 4px;
    color: #ffffff;
    font-size: 14px;
}

/* Labels */
QLabel {
    color: #ffffff;
}

QLabel[font-style=&quot;italic&quot;] {
    color: #8b92a7;
    font-style: italic;
}

/* Combo Boxes */
QComboBox {
    background-color: #2a2d3e;
    border: 1px solid #353847;
    border-radius: 8px;
    padding: 8px 12px;
    color: #ffffff;
    selection-background-color: #00d4ff;
    min-height: 32px;
    font-size: 14px;
}

QComboBox:hover {
    border-color: #00d4ff;
}

QComboBox::drop-down {
    border: none;
}

QComboBox::down-arrow {
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    border-top: 5px solid #8b92a7;
    margin-right: 10px;
}

QComboBox QAbstractItemView {
    background-color: #2a2d3e;
    border: 1px solid #353847;
    border-radius: 8px;
    color: #ffffff;
    selection-background-color: #00d4ff;
}

/* Spin Box */
QSpinBox {
    background-color: #2a2d3e;
    border: 1px solid #353847;
    border-radius: 8px;
    padding: 8px 12px;
    color: #ffffff;
    min-height: 32px;
    font-size: 14px;
}

QSpinBox:hover {
    border-color: #00d4ff;
}

QSpinBox::up-button, QSpinBox::down-button {
    background-color: #353847;
    border: none;
    border-radius: 4px;
    width: 24px;
}

/* Sliders */
QSlider::groove:horizontal {
    height: 8px;
    background: #2a2d3e;
    border-radius: 4px;
}

QSlider::handle:horizontal {
    background: #00d4ff;
    width: 24px;
    height: 24px;
    margin: -8px 0;
    border-radius: 12px;
}

QSlider::handle:horizontal:hover {
    background: #00b8e6;
}

QSlider::sub-page:horizontal {
    background: #00d4ff;
    border-radius: 4px;
}

/* Check Boxes */
QCheckBox {
    color: #ffffff;
    spacing: 12px;
    font-size: 14px;
}

QCheckBox::indicator {
    width: 22px;
    height: 22px;
    border: 2px solid #353847;
    border-radius: 6px;
    background-color: #2a2d3e;
}

QCheckBox::indicator:checked {
    background-color: #00d4ff;
    border-color: #00d4ff;
}

QCheckBox::indicator:hover {
    border-color: #00d4ff;
}

/* Buttons */
QPushButton {
    border-radius: 8px;
    font-size: 14px;
    font-weight: normal;
    padding: 12px 24px;
}

QPushButton#uploadFileButton {
    background-color: #2a2d3e;
    color: #ffffff;
    border: 1px solid #353847;
    min-height: 32px;
}

QPushButton#uploadFileButton:hover {
    background-color: #353847;
    border-color: #00d4ff;
}

QPushButton#uploadFileButton:disabled {
    background-color: #1f2333;
    color: #5a5f6e;
    border-color: #2a2d3e;
}

QPushButton#backButton {
    background-color: #2a2d3e;
    color: #ffffff;
    border: 1px solid #353847;
    min-width: 120px;
    min-height: 35px;
}

QPushButton#backButton:hover {
    background-color: #353847;
    border-color: #ff6b6b;
}

QPushButton#startAnalysisButton {
    background-color: #00d4ff;
    color: #ffffff;
    border: none;
    font-weight: bold;
    min-width: 200px;
    min-height: 35px;
}

QPushButton#startAnalysisButton:hover {
    background-color: #00b8e6;
}

/* Scroll Area */
QScrollArea {
    border: none;
    background-color: transparent;
}

QScrollArea &gt; QWidget &gt; QWidget {
    background-color: transparent;
}

QScrollBar:vertical {
    background-color: #2a2d3e;
    width: 10px;
    border-radius: 5px;
}

QScrollBar::handle:vertical {
    background-color: #353847;
    border-radius: 5px;
    min-height: 20px;
}

QScrollBar::handle:vertical:hover {
    background-color: #00d4ff;
}

/* Value Labels */
QLabel.valueLabel {
    color: #00d4ff;
    font-size: 14px;
    font-weight: bold;
}

/* Total Percentage Label */
#totalPercentageLabel {
    color: #4ade80;
    font-size: 14px;
    font-weight: bold;
}

/* Settings Button */
QPushButton#settingsButton {
    background-color: transparent;
    border: none;
    color: #8b92a7;
    font-size: 20px;
}

QPushButton#settingsButton:hover {
    color: #ffffff;
}

/* Grid Layout Spacing */
QGridLayout {
    spacing: 15px;
}</string>
