/* Main Window - Unified Deep Space Background */
QMainWindow {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #0d1117,
        stop:0.5 #151b25,
        stop:1 #0d1117
    );
}

/* Sidebar - Same Background, Subtle Border */
#sidebar {
    background: transparent;
    border-right: 1px solid rgba(0, 212, 255, 0.15);
}

/* Sidebar List - Interactive Glass Items */
QListWidget#sidebarList {
    background-color: transparent;
    border: none;
    outline: none;
    color: rgba(160, 180, 210, 0.85);
    font-size: 14px;
    font-weight: 500;
    padding: 10px 5px;
}

QListWidget#sidebarList::item {
    background: transparent;
    border: 1px solid transparent;
    border-radius: 10px;
    color: rgba(160, 180, 210, 0.8);
    padding: 14px 18px;
    margin: 3px 8px;
    height: 22px;
}

QListWidget#sidebarList::item:hover {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:0,
        stop:0 rgba(0, 212, 255, 0.15),
        stop:0.8 rgba(0, 212, 255, 0.05),
        stop:1 transparent
    );
    border: 1px solid rgba(0, 212, 255, 0.25);
    border-left: 3px solid rgba(0, 212, 255, 0.6);
    color: #ffffff;
}

QListWidget#sidebarList::item:selected {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:0,
        stop:0 rgba(0, 212, 255, 0.25),
        stop:0.6 rgba(0, 212, 255, 0.1),
        stop:1 transparent
    );
    border: 1px solid rgba(0, 212, 255, 0.4);
    border-left: 4px solid #00d4ff;
    color: #00d4ff;
    font-weight: 600;
}

/* Content Area - Transparent */
#contentArea {
    background: transparent;
}

/* Welcome Title */
#welcomeTitle {
    color: #ffffff;
    font-size: 38px;
    font-weight: 700;
    letter-spacing: -0.5px;
}

#welcomeSubtitle {
    color: rgba(160, 180, 210, 0.8);
    font-size: 16px;
}

/* Primary Button - Glowing Cyan */
QPushButton#primaryButton {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #00d4ff,
        stop:0.5 #00b8e6,
        stop:1 #0099cc
    );
    color: #ffffff;
    border: none;
    border-radius: 12px;
    padding: 15px 40px;
    font-size: 15px;
    font-weight: 600;
}

QPushButton#primaryButton:hover {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #33dfff,
        stop:0.5 #00c8f5,
        stop:1 #00a8dd
    );
}

QPushButton#primaryButton:pressed {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #00a8dd,
        stop:1 #0088bb
    );
}

/* Secondary Button - Frosted Glass */
QPushButton#secondaryButton {
    background: rgba(255, 255, 255, 0.06);
    color: rgba(220, 230, 245, 0.9);
    border: 1px solid rgba(255, 255, 255, 0.12);
    border-radius: 12px;
    padding: 15px 40px;
    font-size: 15px;
    font-weight: 500;
}

QPushButton#secondaryButton:hover {
    background: rgba(0, 212, 255, 0.12);
    border: 1px solid rgba(0, 212, 255, 0.35);
    color: #ffffff;
}

/* Cards - Frosted Glass Panels */
QFrame#card, QFrame#card_2, QFrame#card_3 {
    background: rgba(255, 255, 255, 0.04);
    border: 1px solid rgba(255, 255, 255, 0.08);
    border-radius: 16px;
    padding: 20px;
}

QFrame#card:hover, QFrame#card_2:hover, QFrame#card_3:hover {
    background: rgba(0, 212, 255, 0.06);
    border: 1px solid rgba(0, 212, 255, 0.25);
}

/* Card Titles */
QLabel#cardTitle, QLabel#cardTitle_2, QLabel#cardTitle_3, QLabel#cardTitle_4 {
    color: #ffffff;
    font-size: 15px;
    font-weight: 600;
}

/* Card Text */
QLabel#cardText, QLabel#cardText_2, QLabel#cardText_3, QLabel#cardText_4, QLabel#cardText_5, QLabel#cardText_6 {
    color: rgba(160, 180, 210, 0.75);
