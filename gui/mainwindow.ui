}

/* Settings Button - Icon Glow */
QPushButton#settingsButton {
    background-color: transparent;
    border: none;
    border-radius: 10px;
    padding: 6px;
}

QPushButton#settingsButton:hover {
    background: rgba(0, 212, 255, 0.12);
}
